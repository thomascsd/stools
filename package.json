{
  "name": "@thomascsd/stools",
  "version": "1.0.4",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/thomascsd/stools"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "deploy": "npm publish --access public"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "thomascsd",
  "module": "dist/stools.esm.js",
  "size-limit": [
    {
      "path": "dist/stools.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/stools.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "dotenv": "^8.2.0",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "asyncairtable": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  }
}
