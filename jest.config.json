{
  "verbose": true,
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "collectCoverageFrom": ["src/**/*.{js,ts}", "!<rootDir>/node_modules/"],
  "transformIgnorePatterns": [
    "node_modules/(?!(got)/)"
  ],
  "moduleNameMapper": {
    "^got$": "<rootDir>/node_modules/got"
  }
}